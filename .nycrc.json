{
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "**/*.test.ts"
  ],
  "extension": [
    ".ts"
  ],
  "reporter": [
    "text-summary",
    "json"
  ],
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "check-coverage": true,
  "per-file": true,
  "cache": false,
  "all": true,
  "colors": true,
  "recursive": true
}